	<simpleType name="UomDistanceType">
		<annotation>
			<appinfo>
				<gml:description>A unit of measurement for a horizontal distance.
For example, metres, feet, nautical miles, kilometres, etc...</gml:description>
			</appinfo>
		</annotation>
		<union>
			<simpleType>
				<restriction base="string">
					<enumeration value="NM">
						<annotation>
							<appinfo>
								<gml:description>Nautical miles.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="KM">
						<annotation>
							<appinfo>
								<gml:description>Kilometres.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="M">
						<annotation>
							<appinfo>
								<gml:description>Metres.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="FT">
						<annotation>
							<appinfo>
								<gml:description>Feet.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="MI">
						<annotation>
							<appinfo>
								<gml:description>Statute Mile</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
					<enumeration value="CM">
						<annotation>
							<appinfo>
								<gml:description>Centimetre.</gml:description>
							</appinfo>
						</annotation>
					</enumeration>
				</restriction>
			</simpleType>
			<simpleType>
				<restriction base="string">
					<pattern value="OTHER(:(\w|_){1,58})?"/>
				</restriction>
			</simpleType>
		</union>
	</simpleType>
	<simpleType name="UomDistanceVerticalType">
